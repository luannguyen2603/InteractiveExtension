{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "manifest_version": 2,
    "name": "Random Quotes For Random Search",
    "short_name": "Random Quotes",
    "version": "1.0",
    "description": "This extension gets you new tab with interactive background and random quotes.",
    
    "chrome_url_overrides": {
