    "content_security_policy":"script-src 'self' https://code.jquery.com/jquery-2.2.4.min.js https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/js/materialize.min.js https://en.wikiquote.org/w/api.php https://cldup.com/S6Ptkwu_qA.js; object-src 'self'"
}